{
	"name": "mamd-analytical",
	"productName": "MaMD Analytical",
	"version": "0.3.5",
	"description": "MaMDAnalytical uses an artificial neural network (aNN) to classify an unknown cranium into a reference group.",
	"license": "",
	"repository": "rer145/mamd-analytical",
	"author": {
		"name": "Ron Richardson",
		"email": "ron.richardson@gmail.com"
	},
	"scripts": {
		"postinstall": "electron-builder install-app-deps",
		"lint": "xo",
		"test": "npm run lint",
		"start": "electron .",
		"pack": "electron-builder --dir",
		"dist": "electron-builder --macos --windows -p always",
		"dist-win": "electron-builder --windows",
		"dist-mac": "electron-builder --macos",
		"release": "np"
	},
	"dependencies": {
		"bootstrap": "^4.3.1",
		"chart.js": "^2.9.2",
		"chartjs-plugin-colorschemes": "^0.4.0",
		"electron-context-menu": "^0.12.1",
		"electron-debug": "^3.0.0",
		"electron-log": "^3.0.9",
		"electron-settings": "^3.2.0",
		"electron-store": "^3.3.0",
		"electron-unhandled": "^2.2.0",
		"electron-updater": "^4.0.6",
		"electron-util": "^0.11.0",
		"electron-window-state": "^5.0.3",
		"find": "^0.3.0",
		"fs": "0.0.1-security",
		"jquery": "^3.4.1",
		"popper.js": "^1.15.0",
		"sudo-prompt": "^9.0.0",
		"tether": "^1.4.6"
	},
	"devDependencies": {
		"@fortawesome/fontawesome-free": "^5.9.0",
		"electron": "^5.0.3",
		"electron-builder": "^20.43.0",
		"np": "^5.0.3",
		"xo": "^0.24.0"
	},
	"xo": {
		"envs": [
			"node",
			"browser"
		],
		"rules": {
			"no-multi-assign": "off"
		}
	},
	"np": {
		"publish": true,
		"releaseDraft": true
	},
	"build": {
		"appId": "edu.msu.MaMDAnalytical",
		"publish": [
			{
				"provider": "github",
				"owner": "rer145",
				"repo": "mamd-analytical"
			}
		],
		"win": {
			"icon": "assets/icons/mamd.png"
		},
		"mac": {
			"category": "public.app-category.education",
			"darkModeSupport": false,
			"icon": "assets/icons/mamd.png"
		},
		"dmg": {
			"iconSize": 160,
			"contents": [
				{
					"x": 180,
					"y": 170
				},
				{
					"x": 480,
					"y": 170,
					"type": "link",
					"path": "/Applications"
				}
			]
		}
	}
}
